<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/malaysia_bg"
    tools:context=".TemaActivity">

    <TextView
        android:id="@+id/textView32"
        android:layout_width="331dp"
        android:layout_height="179dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="25dp"
        android:text="@string/tema_txt_1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="283dp"
        android:layout_height="236dp"
        android:layout_alignEnd="@+id/textView32"
        android:layout_alignParentTop="true"
        android:layout_marginTop="218dp"
        android:layout_marginEnd="28dp"
        app:srcCompat="@drawable/georgetown" />

    <Button
        android:id="@+id/button12"
        android:layout_width="401dp"
        android:layout_height="72dp"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-60dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/btn_kosong_biru"
        android:onClick="goToGeorgetown"
        android:text="Georgetown"
        android:textColor="@color/common_google_signin_btn_text_dark_pressed" />

</RelativeLayout>